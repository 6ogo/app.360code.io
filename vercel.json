{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/styles/(.*)",
      "dest": "/public/styles/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/ public/js/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/public/api/$1"
    },
    {
      "src": "/(.*\\.(svg|png|jpg|jpeg|ico|json))",
      "dest": "/public/$1" 
    },
    {
      "src": "/auth",
      "dest": "/server.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}